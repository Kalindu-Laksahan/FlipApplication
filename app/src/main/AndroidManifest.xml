<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodorder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SocialMediaApp.PostDetailActivity"></activity>
        <activity android:name="com.example.SocialMediaApp.AddPostActivity" />
        <activity
            android:name="com.example.SocialMediaApp.ChatActivity"
            android:theme="@style/AppThemeNo" />
        <activity android:name="com.example.SocialMediaApp.LoginActivity" />
        <activity android:name="com.example.SocialMediaApp.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SocialMediaApp.RegisterActivity" />
        <activity android:name="com.example.SocialMediaApp.MainActivity" />
    </application>

</manifest>